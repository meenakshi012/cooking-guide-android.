<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:startColor="#FFDED067"
                android:angle="90"
                android:endColor="#FFDED067"
                android:centerColor="#FFDED067"
                android:gradientRadius="90"
                />
            <stroke android:color="#ded067" android:width="2dp"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>


</selector>