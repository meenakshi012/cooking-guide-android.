<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:startColor="#edf4a1"
                android:angle="90"
                android:endColor="#a3720a"
                android:centerColor="#f5c834"
                android:gradientRadius="90"
                />
            <stroke android:color="#916010" android:width="2dp"/>
            <corners android:radius="25dp"/>
        </shape>
    </item>
</selector>